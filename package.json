{
  "name": "setlife-tools",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf public",
    "dev": "cross-os dev:os",
    "dev:os": {
      "darwin": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --config config/webpack.config.dev.js",
      "win32": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --config config/webpack.config.dev.js"
    },
    "lint": "eslint ./web",
    "prebuild": "rm -rf public && mkdir public",
    "build": "./node_modules/webpack/bin/webpack.js --config config/webpack.config.prod.js",
    "start": "node server.js",
    "test": "jest --colors --verbose"
  },
  "jest": {
    "globals": {},
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup/setupFramework.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/tests/setup/",
      "<rootDir>/api/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^scripts(.*)$": "<rootDir>/web/scripts$1",
      "^styles(.*)$": "<rootDir>/web/styles$1",
      "^components(.*)$": "<rootDir>/web/components$1",
      "^reducers(.*)$": "<rootDir>/web/reducers$1",
      "\\.(css|less|png|jpg|jpeg)$": "<rootDir>/tests/setup/mock.js"
    }
  },
  "bin": {
    "setlife": "./setlife.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/setlife-network/setblocks.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
